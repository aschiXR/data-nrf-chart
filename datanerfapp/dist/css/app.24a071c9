body{margin:0;background-color:#f0f0f0}.header{width:100%;height:72px;background-color:#fff}.header h1{width:100%;padding:.5em 0 0 0;font-family:Roboto,sans-serif;font-weight:400;color:#868686;text-align:center}.header h1,.logo{margin:0;position:absolute}.logo{width:42px;height:46px;padding:1em 0 0 10%}.headerB{width:100%;height:48px;background-color:#00857a}@media only screen and (max-width:600px){.logo{display:none}}.customTooltip-wrapper{font-family:Roboto,sans-serif;opacity:0;display:none;position:absolute}.customTooltip{background:#fff;box-shadow:0 0 5px #999;color:#333;position:absolute;font-size:12px;left:12em;text-align:center;top:15em;z-index:10;text-align:left;width:25em;height:13em}.tooltip-hr{width:70px;background-color:#91a4a5;height:1px;margin-left:auto;margin-right:auto}.tooltip-desc{margin:20px}.tooltip-desc .name{color:#868686;font-weight:900;letter-spacing:1px;font-size:24px;font-weight:700;margin-bottom:2px;text-decoration:none}.tooltip-desc .position{color:#59525b;letter-spacing:1px;font-size:17px;font-weight:500;margin-bottom:2px;margin-top:0}.tooltip-desc .area{color:#91a4a5;letter-spacing:1px;font-size:16px;font-weight:400;margin-bottom:2px;margin-top:7px}.bottom-tooltip-hr{width:100%;background-color:#00857a;height:13px;margin-left:auto;margin-right:auto}.nodeDoesNotHaveChildren,.nodeHasChildren{fill:#fff}text{fill:#696969}.link{fill:none;stroke:#ccc;stroke-width:1.5px}.node text{fill:#868686;font-family:Segoe UI,Arial,sans-serif;font-size:11px}.node circle{stroke-width:20px;stroke:#ffd200;fill:#ffd200}.node-group .name{fill:#222;font-size:18px;font-weight:600}.node-group .description{fill:#59525b;font-size:14px}.node-group .parent{fill:#59525b;font-size:11px}.node-group .count{fill:#91a4a5;font-size:12px}
